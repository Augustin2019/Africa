	function ActiveManageActionLinks() {

		//Delete website
		$(".confirm-website-remove").on('click', function() {

			var $this = $(this);

			bootbox.confirm({
				message: translations.deleteWebsiteConfirmMsg.replace('{{websiteName}}',$(this).data("website-name")),
				buttons: {
					confirm: {
						label: "OK",
						className: 'btn-danger btn-sm'
					},
					cancel: {
						label: "Annuler",
						className: 'btn-sm'
					}
				},
				callback: function(result) {
					if(result) {
						location.href = $this.data("href");
					}
				}
			});
		});

		//Restore website
		$(".confirm-website-restore").on('click', function() {

			var $this = $(this);

			bootbox.confirm({
				message: translations.restoreWebsiteConfirmMsg.replace('{{websiteName}}',$(this).data("website-name")),
				buttons: {
					confirm: {
						label: "OK",
						className: 'btn-danger btn-sm'
					},
					cancel: {
						label: "Annuler",
						className: 'btn-sm'
					}
				},
				callback: function(result) {
					if(result) {
						location.href = $this.data("href");
					}
				}
			});
		});

		//Duplicate website
		$(".confirm-website-duplicate").on('click', function() {

			var $this = $(this);

			bootbox.confirm({
				message: translations.duplicateWebsiteConfirmMsg.replace('{{websiteName}}',$this.data("website-name")),
				buttons: {
					confirm: {
						label: "OK",
						className: 'btn-danger btn-sm'
					},
					cancel: {
						label: "Annuler",
						className: 'btn-sm'
					}
				},
				callback: function(result) {
					if(result) {
						DuplicateWebsite($this.data("website-id"),$this.data("version"),$this.data("website-name"));
					}
				}
			});
		});

		//Reset website
		$(".confirm-website-reset").on('click', function() {

			var $this = $(this);

			//Get website ID
			var websiteID = $this.data('website-id');
			var version = $this.data('version');

			bootbox.confirm({
				title: "\u00cates-vous certain de vouloir r\u00e9initialiser\u00a0?",
				message: "\"R\u00e9initialiser\" changera vos modifications \u00e0 la derni\u00e8re version publi\u00e9e.",
				buttons: {
					confirm: {
						label: "OK",
						className: 'btn-danger',
					},
					cancel: {
						label: "Annuler",
						className: '',
					}
				},
				callback: function(result) {

					if (result==true) {

						//Make the reset
						if( result ) {
							$.ajax({
								type: "POST",
								url: "/versions/"+version+"/wizard/reset.php",
								data: 'w='+websiteID+'', // serializes the form's elements.
								success: function(data) {
									location.reload();
								}
							});
						}
					}
				}
			});
		});

		/**
		 * Move To Expert - Experts customers can convert every website under
		 * there account to be under there expert account.
		 */
		$(".confirm-move-to-expert").on('click', function() {
			// get objects
			var $this = $(this);
			var websiteID = $this.data('website-id');
			var package = $this.data('package');
			var partnerType = $this.data('partnerType');
			var msg = $this.data('msg');
			// security - an expert can't connect a paid website into his account
			if ( package != '1' ) {
				if ( msg == 'disconnect' ) {
					bootbox.alert("Site web payant - D\u00e9sol\u00e9, vous ne pouvez pas d\u00e9connecter un site web payant de votre compte Expert.");
				} else {
					bootbox.alert("Site Web payant - D\u00e9sol\u00e9, vous ne pouvez pas connecter un site Web payant \u00e0 votre compte expert.");
				}
				return;
			}
			// check if the website is already connected to an expert account
			if ( partnerType == 'expert' ) {
				// expert disconnect user confirmations
				bootbox.confirm({
					title: "Confirmation de la D\u00e9connexion de l'Expert",
					message: "\u00cates-vous s\u00fbr de vouloir d\u00e9connecter ce site Web de votre compte Expert ?",
					buttons: {
						confirm: {
							label: "OK",
							className: 'btn-primary',
						},
						cancel: {
							label: "Annuler",
							className: '',
						}
					},
					callback: function( confirm ) {
						// exit - the user didn't confirm
						if ( !confirm ) return;
						// connect the website to the user expert account
						$.post('/versions/2/wizard/expert_disconnect.php', {
							w: websiteID,
						}).done(function( response ) {
							if ( response === 'Success' ) {
								topWindow.location.reload();
							} else {
								bootbox.alert("\u00c9chec de la d\u00e9connexion de l'expert - Le syst\u00e8me n'a pas r\u00e9ussi \u00e0 d\u00e9connecter le site Web de votre compte expert. Veuillez contacter notre \u00e9quipe de support.");
							}
						});
					}
				});
			} else {
				// expert connect user confirmations
				bootbox.confirm({
					title: "Connexion d'Expert Confirm\u00e9e",
					message: "\u00cates-vous s\u00fbr de vouloir connecter ce site Web \u00e0 votre compte Expert ?",
					buttons: {
						confirm: {
							label: "OK",
							className: 'btn-primary',
						},
						cancel: {
							label: "Annuler",
							className: '',
						}
					},
					callback: function( confirm ) {
						// exit - the user didn't confirm
						if ( !confirm ) return;
						// connect the website to the user expert account
						$.post('/versions/2/wizard/expert_connect.php', {
							w: websiteID,
						}).done(function( response ) {
							if ( response === 'Success' ) {
								topWindow.location.reload();
							} else {
								bootbox.alert("\u00c9chec de la connexion expert - Le syst\u00e8me n'a pas r\u00e9ussi \u00e0 connecter le site Web \u00e0 votre compte expert. Veuillez contacter notre \u00e9quipe de support.");
							}
						});
					}
				});
			}
		});
	}

	function DuplicateWebsite(websiteID,versionNUM,websiteName) {

		//Get instance of progressbar object
		var progressbar = new Progressbar("Votre site web se duplique, veuillez patienter.");

		//Start the progress bar
		progressbar.start(250);

		//Create a new website via Ajax
		var request = $.post(
			'/versions/'+versionNUM+'/wizard/duplicateWebsite.php', {
			websiteID : websiteID,
            name : websiteName
		}).done(function( response ) {
			response                = jQuery.parseJSON(response);
			var newWebsiteID        = response.newWebsiteID;
			var websiteSubdomain    = response.websiteSubdomain;

			if (jQuery.isNumeric(newWebsiteID)) {
				mixPanelEvent(false,"Duplicate Website");
				progressbar.finish(function() {
					window.top.location.href = '/manager/wizard.php?w=' + newWebsiteID + '&from=dash&new=1';
				});
			} else {
				progressbar.stop();
			}
		});
	}

	function ActiveAccessBlockedMessage() {
		// display access blocked message
		$('.access-blocked-message').on('click', function() {
			// get object
			$this = $(this);
			msg = $this.data('access-blocked-msg');
			// initialize access blocked message
			var html = '<div style="margin-top:30px;"><b>' + msg + '</b></div>';
			// initialize sweet alert
			swal({
				icon: 'error',
				content: $(html).get(0),
				buttons: "OK"			});
		});
	}
	// Dashboard notification will notify the user on changes in the dashboard 
	function getDashboardnotifications() {

		// If the user is not active in the wizard right now we not running this AJAX
        if (userActive_isActive == false) return;

        // Count the notification number via Ajax
        $.post( '/versions/'+versionNUM+'/wizard/include/websiteNotificationsV_beta_V4.php', {
            w: websiteID,
            type: 'total'
        }).done( function( data ) {

            // prevent JS error in case the JSON didn't returned
            try { 
                data = JSON.parse(data);
            } catch (e) {}

            if ( !data || typeof data !== "object" ) return;
            // check if the user package changed if it did will alert him to refresh
            if( data.packageNUM != packageNUM && !window.ignorePackageChangedMessage && !isBsModalOpen('#packageChangedModal') ) {
                var packageChangedModal = bootbox.confirm({
                    title: '<span><b>'+translations.packageChangeTitle+'</b></span>',
                    message: translations.packageChangeMessageDashboard,
                    buttons: {
                        confirm: {
                            label: translations.yes,
                            className: 'btn-success'
                        },
                        cancel: {
                            label: translations.no,
                            className: 'btn-default'
                        }
                    },
                    callback: function( result ) {
                        // refreshing the page 
                        if ( result == true ) {
                            window.location.reload();
                        // update flag ignore package changed message because the user click on No button
                        } else if ( result == false ) {
                            window.ignorePackageChangedMessage = true;
                        }
                    }
                });
                // add ID on the package changed modal
                packageChangedModal.on("shown.bs.modal", function() {
                    packageChangedModal.attr("id", "packageChangedModal");
                });
            }
        });
    }
    function userActive_ShowTimeOutWarning() {
    	userActive_isActive = false;
	}
	//We force the website notification every 5min so if the user back to is screen he will see an update
	function userActive_ForceWebsiteNotifications() {
    	userActive_isActive = true;
    	getDashboardnotifications();
	}	 

</script>


